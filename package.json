{
  "name": "mobx-stored",
  "version": "0.9.0",
  "description": "localstorage persisted observables",
  "main": "stored-observable.js",
  "module": "src/stored-observable.js",
  "scripts": {
    "build": "babel ./src/stored-observable.js --out-file stored-observable.js",
